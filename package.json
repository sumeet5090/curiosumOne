{
  "name": "website-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "",
    "start": "concurrently --names \"SERVER,CLIENT\" -c \"bgGreen.bold,bgMagenta.bold\" \"yarn dev\" \"yarn dev: client\"",
    "dev": "nodemon ./bin/www --exec babel-node --preset env --ignore views/",
    "dev:client": "cd views && yarn dev",
    "build:client": "cd views && yarn build",
    "deploy:dev": "concurrently \"yarn dev:client --port=8080\" \"lt -l 0.0.0.0 -p 8080\""
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "connect-history-api-fallback": "^1.5.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.14",
    "mongoose-validator": "^2.1.0",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.11.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "2.0.0-beta11",
    "rand-token": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  }
}
